    "@nestjs/microservices": "^10.3.1",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
